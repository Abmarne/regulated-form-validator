{
  "name": "regulated-form-validator",
  "version": "1.0.2",
  "description": "YAML-driven dynamic form validator and React renderer for regulated domains like BFSI and healthcare.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@rollup/plugin-yaml": "^4.1.2",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "scripts": {
    "build": "tsup src/index.js --format esm,cjs --minify --loader .js=jsx && copy index.d.ts dist\\index.d.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "npm run build",
    "dev": "vite"
  },
  "keywords": [
    "form",
    "validator",
    "yaml",
    "react",
    "BFSI",
    "healthcare",
    "ifsc",
    "pan",
    "aadhaar",
    "compliance",
    "regulated",
    "typescript"
  ],
  "author": "Abhiraj Madan Marne",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abhirajmarne/regulated-form-validator.git"
  },
  "bugs": {
    "url": "https://github.com/abhirajmarne/regulated-form-validator/issues"
  },
  "homepage": "https://github.com/abhirajmarne/regulated-form-validator#readme"
}
